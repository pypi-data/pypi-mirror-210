<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{ project_name }} Packaging By DLRN</title>
    <link rel="stylesheet" href="{{ url_for('static', filename='styles.css') }}">
  </head>
  <body>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.js" integrity="sha384-qPGCkCPOuGBUt/edB/0Dab3wE62dCo5HskKmbabea0YxTq2fhWoKXO8ItrP6qZu+" crossorigin="anonymous"></script>
    <script src="{{ url_for('static', filename='utils.js') }}"></script>
    <h1>
      <i class='fa fa-chevron-circle-right pull-left'></i>
      {{ reponame | capitalize }} - {{ target | capitalize }} ({{ src }})
    </h1>
    <div id="navigation1"></div>
    <br>
    <table id="dlrn" class="pf-m-compact pf-m-grid-md cell-border hover" role="grid" aria-label="dlrntable">
        <thead>
            <tr>
                <th>Build Date Time</th>
                <th>Commit Date Time</th>
                <th>Project Name</th>
                <th>Version</th>
                <th>Release</th>
                <th>Commit Hash</th>
                <th>Component</th>
                <th>Status</th>
                <th>Repository</th>
                <th>Build Log</th>
            </tr>
        </thead>
    </table>
    <script>
        var baseurl = "{{ baseurl }}";
        $(document).ready( reportHtml );
    </script>
  </body>
</html>
