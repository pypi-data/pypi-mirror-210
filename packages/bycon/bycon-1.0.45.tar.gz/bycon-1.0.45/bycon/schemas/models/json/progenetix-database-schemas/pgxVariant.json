{
    "$id": "https://progenetix.org/services/schemas/pgxVariant/v2023-05-24",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "definitions": {
        "Location": {
            "properties": {
                "interval": {
                    "$ref": "#/definitions/SequenceInterval"
                },
                "sequenceId": {
                    "type": "string"
                },
                "type": {
                    "const": "SequenceLocation"
                }
            },
            "type": "object"
        },
        "PointPosition": {
            "properties": {
                "type": {
                    "const": "Number"
                },
                "value": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "SequenceInterval": {
            "properties": {
                "end": {
                    "$ref": "#/definitions/PointPosition"
                },
                "start": {
                    "$ref": "#/definitions/PointPosition"
                },
                "type": {
                    "const": "SequenceInterval"
                }
            },
            "type": "object"
        },
        "SequenceState": {
            "properties": {
                "const": "LiteralSequenceExpression"
            },
            "sequence": {
                "type": "string"
            },
            "type": "object"
        }
    },
    "description": "Schema for a genomic variant in Progenetix collections.",
    "properties": {
        "alternateBases": {
            "pattern": "^([ACGTUNRYSWKMBDHV\\-\\.]*)$",
            "type": "string"
        },
        "biosampleId": {
            "type": "string"
        },
        "callsetId": {
            "type": "string"
        },
        "end": {
            "type": "integer"
        },
        "id": {
            "type": "string"
        },
        "individualId": {
            "type": "string"
        },
        "info": {
            "type": "object"
        },
        "location": {
            "$ref": "#/definitions/Location"
        },
        "referenceBases": {
            "pattern": "^([ACGTUNRYSWKMBDHV\\-\\.]*)$",
            "type": "string"
        },
        "relativeCopyClass": {
            "enum": [
                "copy neutral",
                "partial loss",
                "complete loss",
                "low-level gain",
                "high-level gain"
            ],
            "type": "string"
        },
        "start": {
            "type": "integer"
        },
        "state": {
            "$ref": "#/definitions/SequenceState"
        },
        "type": {
            "enum": [
                "Allele",
                "RelativeCopyNumber"
            ],
            "type": "string"
        },
        "updated": {
            "examples": [
                "2020-09-10T17:44:10.170Z"
            ],
            "format": "date-time",
            "type": "string"
        },
        "variantInternalId": {
            "type": "string"
        },
        "variantState": {
            "$ref": "../progenetix-model/common/ontologyTerm.json"
        },
        "variantType": {
            "type": "string"
        }
    },
    "required": [
        "variantInternalId",
        "variantType"
    ],
    "title": "Progenetix Variant",
    "type": "object"
}