L=subgrouplist(bnr, [3], 1);
setrand(1);filter(bnrclassfield(bnr, L, 0))
setrand(1);filter(bnrclassfield(bnr, L, 1))
setrand(1);filter(bnrclassfield(bnr, L, 2))
bnrclassfield(bnr,[])
