print("vector of subgroups"); \\ avoid 32-bit differences
deg(a)=poldegree(if (type(a)=="t_POL", a, a[1]));
filter(v)=[if (cmp(a,b=(-1)^deg(a)*subst(a,x,-x)) < 0,b,a)| a<-v];
